{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifydatafabrica"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"Ls_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ls_ADLS'"
		},
		"Ls_AzureSqlDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ls_AzureSqlDB'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dexter.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyDatabase"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dexter"
		},
		"Ls_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyadls1001.dfs.core.windows.net/"
		},
		"Ls_AzureSqlDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dexter.database.windows.net"
		},
		"Ls_AzureSqlDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyDatabase"
		},
		"Ls_AzureSqlDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dexter"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifydatafabrica_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"Ls_ADLS_accountKey": {
						"value": "[parameters('Ls_ADLS_accountKey')]"
					},
					"Ls_AzureSqlDB_password": {
						"value": "[parameters('Ls_AzureSqlDB_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Ls_ADLS_properties_typeProperties_url": {
						"value": "[parameters('Ls_ADLS_properties_typeProperties_url')]"
					},
					"Ls_AzureSqlDB_properties_typeProperties_server": {
						"value": "[parameters('Ls_AzureSqlDB_properties_typeProperties_server')]"
					},
					"Ls_AzureSqlDB_properties_typeProperties_database": {
						"value": "[parameters('Ls_AzureSqlDB_properties_typeProperties_database')]"
					},
					"Ls_AzureSqlDB_properties_typeProperties_userName": {
						"value": "[parameters('Ls_AzureSqlDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}